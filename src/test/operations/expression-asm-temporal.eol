@cached
operation ASM!EDataType isDate() : Boolean {
    return "EDate".getDataTypeByName() == self
        or "JDate".getDataTypeByName() == self;
}

@cached
operation ASM!EDataType isTimestamp() : Boolean {
    return "JTimestamp".getDataTypeByName() == self;
}

