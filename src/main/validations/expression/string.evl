context EXPR!StringAttribute {
    constraint AttributeTypeIsString {
        guard: self.satisfiesAll("AttributeTypeIsDefined") and self.entityExpression.satisfiesAll("EntityTypeIsDefined")
        
        check: self.getAttributeType().isString()
        message: "Attribute type of " + self.attributeName + " of entity type " + self.entityExpression.getEntityType().getName() + " is not string"
    }
}
