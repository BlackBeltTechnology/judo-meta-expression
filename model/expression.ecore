      <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1"
          eType="#//LogicalExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="operation" lowerBound="1"
          eType="#//operator/LogicalOperator"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="InstanceOfExpression" eSuperTypes="#//LogicalExpression">
