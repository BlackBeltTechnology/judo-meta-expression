          eType="#//EntityExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="entityName" lowerBound="1"
          eType="#//EntityName"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LogicalAttribute" eSuperTypes="#//LogicalExpression #//Attribute"/>
    <eClassifiers xsi:type="ecore:EClass" name="UndefinedAttributeComparison" eSuperTypes="#//LogicalExpression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="entityExpression" lowerBound="1"
          eType="#//EntityExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="attributeName" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" lowerBound="1"
          eType="#//operator/GenericComparator"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="UndefinedNavigationComparison" eSuperTypes="#//LogicalExpression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="entityExression" lowerBound="1"
          eType="#//EntityExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" lowerBound="1"
          eType="#//operator/GenericComparator"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LogicalVariableReference" eSuperTypes="#//LogicalExpression #//VariableReference">
      <eStructuralFeatures xsi:type="ecore:EReference" name="variable" lowerBound="1"
          eType="#//variable/LogicalVariable"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="string" nsURI="http://blackbelt.hu/judo/meta/expression/string">
    <eClassifiers xsi:type="ecore:EClass" name="StringAttribute" eSuperTypes="#//StringExpression #//Attribute"/>
