@namespace(uri="http://blackbelt.hu/judo/meta/expression/evaluation", prefix="eval")
package evaluation;

import "http://blackbelt.hu/judo/meta/expression/temporal";

class EvaluationNode {
  ref expression.Expression[1] expression;
  attr String description;
}

class NavigationNode extends EvaluationNode {
  attr String baseExpression;
  val ProjectionNode[*] projections;
  val NavigationNode navigation;
  val FilteringNode[*] filtering;
  val OrderingNode[*] ordering;
  val WindowingNode[*] windowing;
}

class FilteringNode extends EvaluationNode {
}

class OrderingNode extends EvaluationNode {
}

class WindowingNode extends EvaluationNode {
}

class ProjectionNode extends EvaluationNode {
}

