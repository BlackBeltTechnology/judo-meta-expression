operation ASM!EClass getReference(name: String) : ASM!EReference {
    return modelAdapter.getReference(self, name).orElse(null);
}

operation ASM!EClass getAttribute(name: String) : ASM!EAttribute {
    return modelAdapter.getAttribute(self, name).orElse(null);
}

operation ASM!EClass getAttributeType(name: String) : ASM!EClassifier {
    return modelAdapter.getAttributeType(self, name).orElse(null);
}
